{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleDirectories": ["src"],
  "rootDir": "src",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "../coverage",
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageReporters": ["json-summary", "text"]
}
